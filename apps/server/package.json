{
	"name": "server",
	"main": "src/index.ts",
	"type": "module",
	"scripts": {
		"build": "tsdown",
		"check-types": "tsc -b",
		"compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile server",
		"dev": "wrangler dev --port 3000",
		"start": "wrangler dev",
		"deploy": "wrangler deploy --env production",
		"cf-typegen": "wrangler types --env-interface CloudflareBindings"
	},
	"dependencies": {
		"@better-auth-cloudflare-starter/api": "workspace:*",
		"@better-auth-cloudflare-starter/auth": "workspace:*",
		"@better-auth-cloudflare-starter/db": "workspace:*",
		"@hono/trpc-server": "^0.4.2",
		"@trpc/server": "catalog:",
		"better-auth": "catalog:",
		"dotenv": "catalog:",
		"hono": "catalog:",
		"hono-rate-limiter": "^0.5.3",
		"zod": "catalog:"
	},
	"devDependencies": {
		"typescript": "catalog:",
		"tsdown": "^0.20.3",
		"@better-auth-cloudflare-starter/config": "workspace:*",
		"wrangler": "^4.63.0",
		"@types/node": "catalog:"
	}
}
