{
	"name": "better-auth-cloudflare-starter",
	"private": true,
	"type": "module",
	"workspaces": {
		"packages": [
			"apps/*",
			"packages/*"
		],
		"catalog": {
			"dotenv": "^17.2.4",
			"zod": "^4.3.6",
			"typescript": "^5",
			"@types/node": "^25.2.2",
			"hono": "^4.11.9",
			"@trpc/server": "^11.9.0",
			"better-auth": "^1.4.18",
			"@trpc/client": "^11.9.0",
			"@polar-sh/better-auth": "^1.8.0",
			"resend": "^6.9.1",
			"@better-auth/expo": "^1.4.18"
		}
	},
	"scripts": {
		"dev": "turbo dev",
		"build": "turbo build",
		"check-types": "turbo check-types",
		"dev:native": "turbo -F native dev",
		"dev:web": "turbo -F web dev",
		"dev:server": "turbo -F server dev",
		"deploy": "turbo deploy",
		"db:push": "turbo -F @better-auth-cloudflare-starter/db db:push",
		"db:studio": "turbo -F @better-auth-cloudflare-starter/db db:studio",
		"db:generate": "turbo -F @better-auth-cloudflare-starter/db db:generate",
		"db:migrate": "turbo -F @better-auth-cloudflare-starter/db db:migrate",
		"check": "biome check --write ."
	},
	"dependencies": {
		"@biomejs/biome": "^2.3.14",
		"dotenv": "catalog:",
		"zod": "catalog:"
	},
	"devDependencies": {
		"@types/node": "catalog:",
		"turbo": "^2.8.3",
		"typescript": "catalog:"
	},
	"packageManager": "bun@1.3.0"
}
